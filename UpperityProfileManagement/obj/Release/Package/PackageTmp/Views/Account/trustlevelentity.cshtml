        $.ajax({
            type: 'POST',
            url: '/Account/GetCountryList',
            contentType: 'application/json; charset=utf-8',
            async: false,
            dataType: 'html',
            success: function (data) {
                $("#entitycountry").html(data);
            },
            error: function (xhr) {
                alert('error');
                alert(xhr.responseText);
            }
        });
    }

