                data: '{invoiceType: 2, invoiceLevel: "' + $("input[name='trustlevel']:checked").val() + '"}',
                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                async: false,
                success: function (data) {

                    window.open(data.invoiceURL, '_blank');
                    checkPaiementReceive(data.invoiceId);


                },
                error: function (xhr) {
                    alert('error');
                    alert(xhr.responseText);
                }
            });
        } else {
        $("#submitter").click();
        }
    }

    function checkPaiementReceive(invoiceId) {

        setTimeout(function () {
            $.ajax({
                type: 'POST',
                url: '/Account/checkPaiementReceive',
                data: '{invoiceId: "' + invoiceId + '"}',
                contentType: 'application/json; charset=utf-8',
                async: false,
                dataType: 'json',
                success: function (data) {
                    //$('#EntityDocumentList').html(data);
                    if (data.isPaid == true) {
                        $("#finalinvoiceId").val(invoiceId);
                        $("#submitter").click();
                    } else {
                        checkPaiementReceive(invoiceId);
                    }
                },
                error: function (xhr) {
                    alert('error');
                    alert(xhr.responseText);
                }
            });
        }, 5000);

    }

    function isRequiredCompleted() {
        var form = document.getElementById('entity-trust-level');
        for (var i = 0; i < form.elements.length; i++) {
            if (form.elements[i].value === '' && form.elements[i].hasAttribute('required')) {
                
                return false;
            }
        }

        return true;
    }

    
    function setRequiredDocument() {
    

        var std = {};
        std.entityTypeId = $('#selectedEntityType').val();
        std.EntityTrustLevelId = $("input[name='trustlevel']:checked").val();

        switch (std.EntityTrustLevelId) {
            case '2':
                $('#directorstable').css('display', 'none');
                $('#stakeholderstable').css('display', 'none');

                $('.directorsReqToggle').prop('required', false);
                $('.stakeholderReqToggle').prop('required', false);
                break;
            case '3':
                $('#directorstable').css('display', '');
                $('#stakeholderstable').css('display', 'none');

                $('.directorsReqToggle').prop('required', true);
                $('.stakeholderReqToggle').prop('required', false);
                break;
            case '4':
                $('#directorstable').css('display', '');
                $('#stakeholderstable').css('display', 'none');

                $('.directorsReqToggle').prop('required', true);
                $('.stakeholderReqToggle').prop('required', false);
                break;
            case '5':
                $('#directorstable').css('display', '');
                $('#stakeholderstable').css('display', '');

                $('.directorsReqToggle').prop('required', true);
                $('.stakeholderReqToggle').prop('required', true);
                break;

        }

        $('#requestedTrustLevel').val(std.EntityTrustLevelId);

        $.ajax({
            type: 'POST',
            url: '/Account/GetSelectedDocumentSet',
            data: '{entityInformation: ' + JSON.stringify(std) + '}',
            contentType: 'application/json; charset=utf-8',
            dataType: 'html',
            success: function (data) {
                $('#EntityDocumentList').html(data);
            },
            error: function (xhr) {
                alert('error');
                alert(xhr.responseText);
            }
        });

    }

