@model UpperityProfileManagement.UpperitySDK.GetUserPersonalInformationResponse

@if (!Model.Error)
{
<table width="430px;">
<tr><th>Upperity Id</th>
<th>First Name</th>
<th>Last Name</th>
<th>Add</th></tr>

<tr><td>@Model.UserInformation.UpperityId</td>
<td>@Model.UserInformation.firstName</td>
<td>@Model.UserInformation.LastName</td>
<td><a href="javascript:AddUserToEntity();">Add User</a></td></tr>
</table>
}
else
{
    <div style="text-align:center;">No result</div>
}