                              }
                                
                            </fieldset>
                            <p>If you require some more information, please refer to the page <a href="http://www.upperity.com/how-it-works/" target="_blank">http://www.upperity.com/how-it-works/</a></p>
                           </div>
                        </div>
                    </div>
                     <input type="hidden" name="finalinvoiceId" id="finalinvoiceId" value="" />
                    <!--END Line0-->
<!--FIN AJOUT-->
                    <!--Line1-->
                    <div class="row">
                        <div class="col-lg-4 col-sm-12">
                            <div class="form-group">
                            <label for="firstname" class="text-info">First Name</label><br>
                            <input type="text" name="firstname" id="firstname" class="form-control" value="@Model.UserInformation.firstName" required>
                           </div>
                        </div>
                        <div class="col-lg-4 col-sm-12">
                            <div class="form-group">
                            <label for="middlename" class="text-info">Middle Name</label><br>
                            <input type="text" name="middlename" id="middlename" class="form-control" value="@Model.UserInformation.middleName">
                            </div>
                        </div>
                        <div class="col-lg-4 col-sm-12">
                            <div class="form-group">
                            <label for="lastname" class="text-info">Last Name</label><br>
                            <input type="text" name="lastname" id="lastname" class="form-control" value="@Model.UserInformation.LastName"  required>
                            </div>
                        </div>
                    </div>
                    <!--END Line1-->
                    <!--Line2-->
                    <div class="row">
                        <div class="col-lg-6 col-sm-12">
                            <div class="form-group">
                            <label for="email" class="text-info">Email</label><span class="plus"><a href="javascript:addEmailField();"><img class="plus" src="/dashboard/assets/plus.png"></a></span><br>
                            <!--input type="text" name="firstname" id="firstname" class="form-control"-->
