                                  <tr>
                                    <th class="tg-0lax">Email ID</th>
                                    <th class="tg-1lax">Type</th>
                                    <th class="tg-2lax">Default</th>
                                    <th class="tg-3lax"></th>
                                  </tr>
                                  @if (Model.UserInformation.UserEmailList.Any())
                                        {
                                            foreach (var email in Model.UserInformation.UserEmailList)
                                            {
                                            <tr>
                                                <td class="tg-0lax">
                                                    <input type="text" name="email_@EmailIndexer" id="email_@EmailIndexer" class="form-control" value="@email.email" required>
                                                </td>
                                                <td class="tg-0lax">
                                                    <select class="mdb-select md-form" name="emailType_@EmailIndexer" id="emailType_@EmailIndexer">
                                                        
                                                       @switch (email.emailType)
                                                        {
                                                            case "0":
                                                                <option value="0" selected>Other</option>
                                                                <option value="1">Personal</option>
                                                                <option value="2">Work</option>
                                                                break;
                                                            case "1":
                                                                <option value="0">Other</option>
                                                                <option value="1" selected>Personal</option>
                                                                <option value="2">Work</option>
                                                                break;
                                                            default:
                                                                <option value="0">Other</option>
                                                                <option value="1">Personal</option>
                                                                <option value="2" selected>Work</option>
                                                                break;
                                                        }
                                                        
                                                    </select>
                                                </td>
                                                <td class="tg-0lax">
                                                @if (email.isDefault)
                                                {
                                                    <input type="radio" name="emailDefault" id="emailIsDefault_@EmailIndexer" class="form-control" value="@EmailIndexer" checked>
                                                }
                                                else
                                                {
                                                    <input type="radio" name="emailDefault" id="emailIsDefault_@EmailIndexer" class="form-control" value="@EmailIndexer">
                                                }
                                                   
                                                </td>
                                                <td class="tg-0lax">
                                                @if (EmailIndexer == 0)
                                                {
                                                    <span>&nbsp;</span>
                                                }
                                                else
                                                {
                                                    <a href="#" onclick="removeCurrentRow(this);">
                                                        <img class="trash" src="/dashboard/assets/trash.jpg"></a>
                                                }
                                                </td>
                                            </tr>
                                                EmailIndexer++;
                                            }
                                        }
                                        else
                                        {
                                            <tr>
                                                <td class="tg-0lax">
                                                    <input type="text" name="email_0" id="email_0" class="form-control" required>
                                                </td>
                                                <td class="tg-0lax">
                                                    <select class="mdb-select md-form" name="emailtype_0" id="emailtype_0">
                                                         <option value="0" selected>Other</option>
                                                        <option value="1">Personal</option>
                                                        <option value="2">Work</option>
                                                    </select>
                                                </td>
                                                <td class="tg-0lax">
                                                    <input type="radio" name="emailDefault" id="emailIsDefault_0" value="0" class="form-control">
                                                </td>
                                                <td class="tg-0lax">
                                                     <span>&nbsp;</span>
                                                </td>
                                            </tr>
                                        }
                                </table>
                            </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-sm-12">
                            <div class="form-group">
                            <label for="phone" class="text-info">Phone</label><span class="plus"><a href="javascript:addPhoneField();"><img class="plus" src="/dashboard/assets/plus.png"></a></span><br>
                            <!--input type="text" name="middlename" id="middlename" class="form-control"-->
                            <div class="tg-wrap"><table id="phones-table" class="tg">
                                  <tr>
                                    <th class="tg-0lax">Phone</th>
                                    <th class="tg-1lax">Type</th>
                                    <th class="tg-2lax">Default</th>
                                    <th class="tg-3lax"></th>
                                  </tr>
                                  @if (Model.UserInformation.phoneNumberList.Any())
                                        {

                                            foreach (var phone in Model.UserInformation.phoneNumberList)
                                            {
                                            <tr>
                                            <td class="tg-0lax">
                                                <input type="text" name="phone_@PhoneIndexer" id="phone_@PhoneIndexer" class="form-control" value="@phone.phoneNumber" required>
                                            </td>
                                            <td class="tg-0lax">
                                                <select class="mdb-select md-form" id="phoneType_@PhoneIndexer" name="phoneType_@PhoneIndexer">
                                                    @switch (phone.phoneType)
                                                    {
                                                        case "0":
                                                             <option value="0" selected>Other</option>
                                                            <option value="1">Mobile</option>
                                                            <option value="2">Work</option>
                                                            <option value="3">Home</option>
                                                            break;
                                                        case "1":
                                                           <option value="0">Other</option>
                                                            <option value="1" selected>Mobile</option>
                                                            <option value="2">Work</option>
                                                            <option value="3">Home</option>
                                                            break;
                                                        case "2":
                                                           <option value="0">Other</option>
                                                            <option value="1">Mobile</option>
                                                            <option value="2" selected>Work</option>
                                                            <option value="3">Home</option>
                                                            break;
                                                        default:
                                                            <option value="0">Other</option>
                                                            <option value="1">Mobile</option>
                                                            <option value="2">Work</option>
                                                            <option value="3" selected>Home</option>
                                                            break;
                                                    }
                                                </select>
                                            </td>
                                            <td class="tg-0lax">
                                                @if (phone.isDefault)
                                                {
                                                    <input type="radio" name="phoneDefault" id="phoneisDefault_@PhoneIndexer" class="form-control" value="@PhoneIndexer" checked>
                                                }
                                                else
                                                {
                                                    <input type="radio" name="phoneDefault" id="phoneisDefault_@PhoneIndexer" class="form-control" value="@PhoneIndexer">
                                                }
                                                
                                            </td>
                                            <td class="tg-0lax">
                                                @if (PhoneIndexer == 0)
                                                {
                                                    <span>&nbsp;</span>
                                                }
                                                else
                                                {
                                                    <a href="#" onclick="removeCurrentRow(this);">
                                                    <img class="trash" src="/dashboard/assets/trash.jpg"></a>
                                                }
                                                
                                            </td>
                                            </tr>
                                                PhoneIndexer++;
                                            }
                                        }
                                  else
                                  {
                                            <tr>
                                            <td class="tg-0lax">
                                                <input type="text" name="phone_0" id="phone_0" class="form-control" required>
                                            </td>
                                            <td class="tg-0lax">
                                                <select class="mdb-select md-form" name="phoneType_0" id="phoneType_0">
                                                     <option value="0" selected>Other</option>
                                                            <option value="1">Mobile</option>
                                                            <option value="2">Work</option>
                                                            <option value="3">Home</option>
                                                </select>
                                            </td>
                                            <td class="tg-0lax">
                                                <input type="radio" name="phoneDefault" id="phoneisDefault_0" class="form-control" value="0">
                                            </td>
                                            <td class="tg-0lax">
                                                 <span>&nbsp;</span>
                                            </td>
                                            </tr>
                                      PhoneIndexer++;
                                  }
                                </table>
                            </div>
                            </div>
                        </div>
                    </div>
                    <!--END Line2-->                    
                    <!--Line3-->
<!-- LIGNES INVERSÉES-->
                    <div class="row">
                        <div class="col-lg-3 col-sm-12">
                            <div class="form-group">
                            <label for="buildingname" class="text-info">Building Name</label><br>
                            <input type="text" name="buildingname" id="buildingname" class="form-control" value="@Model.UserInformation.AddressList[0].BuildingName">
                           </div>
                        </div>
                        <div class="col-lg-3 col-sm-12">
                            <div class="form-group">
                            <label for="buildingnumber" class="text-info">Building Number</label><br>
                            <input type="text" name="buildingnumber" id="buildingnumber" class="form-control" value="@Model.UserInformation.AddressList[0].BuildingNumber">
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-12">
                            <div class="form-group">
                            <label for="streetname" class="text-info">Street Name</label><br>
                            <input type="text" name="streetname" id="streetname" class="form-control" value="@Model.UserInformation.AddressList[0].StreetName">
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-12">
                            <div class="form-group">
                            <label for="unitnumber" class="text-info">Unit Number (e.g. Office/Suite/Floor)</label><br>
                            <input type="text" name="unitnumber" id="unitnumber" class="form-control" value="@Model.UserInformation.AddressList[0].UnitNumber">
                            </div>
                        </div>
                    </div>
                    <!--END Line3-->   
                    <!--Line4-->
                    <div class="row">
                        <div class="col-lg-3 col-sm-6">
                            <div class="form-group">
                            <label for="pobox" class="text-info">PO Box</label><br>
                            <input type="text" name="pobox" id="pobox" class="form-control" value="@Model.UserInformation.AddressList[0].POBox">
                            </div>
                        </div>
                         <div class="col-lg-3 col-sm-6">
                            <div class="form-group">
                            <label for="country" class="text-info">Country</label><br>
                                <select class="mdb-select md-form" id="usercountry" name="usercountry" onchange="getStateList();">
                                  <option value="" disabled selected>---</option>
                                 
                                </select>
                             </div>
                        </div>
                        <div class="col-lg-3 col-sm-6">
                            <div class="form-group">
                            <label for="province" class="text-info">Province/State</label><br>
                                <select class="mdb-select md-form" id="userState" name="userState">
                                  <option value="" disabled selected>---</option>
                                 
                                </select>
                                </div>
                        </div>
                        <div class="col-lg-3 col-sm-6">
                            <div class="form-group">
                            <label for="city" class="text-info">City</label><br>
                            <input type="text" name="city" id="city" class="form-control" value="@Model.UserInformation.AddressList[0].City">
                            </div>
                        </div>

                    </div>
                    <!--END Line4-->
                    <!--Line4-->
                    <div class="row">
                        <div class="col-lg-3 col-sm-6">
                            <div class="form-group">
                            <label for="suburb" class="text-info">Suburb</label><br>
                            <input type="text" name="suburb" id="suburb" class="form-control" value="@Model.UserInformation.AddressList[0].SubUrb">
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-6">
                            <div class="form-group">
                            <label for="postalcode" class="text-info">Postal / ZIP Code</label><br>
                            <input type="text" name="postalcode" id="postalcode" class="form-control" value="@Model.UserInformation.AddressList[0].PostalOrZipCode">
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-6">
                            <div class="form-group">
                            <label for="gender" class="text-info">Gender</label><br>
                                <select class="mdb-select md-form" name="GenderSelection">
                                @switch (Model.UserInformation.gender)
                                {
                                    case "1":
                                          <option value="1" selected>Male</option>
                                          <option value="2">Female</option>
                                          <option value="0">Other</option>
                                        break;
                                    case "2":
                                          <option value="1">Male</option>
                                          <option value="2" selected>Female</option>
                                          <option value="0">Other</option>
                                        break;
                                    case "0":
                                          <option value="1">Male</option>
                                          <option value="2">Female</option>
                                          <option value="0" selected>Other</option>
                                        break;
                                    default:
                                          <option value="1" selected>Male</option>
                                          <option value="2">Female</option>
                                          <option value="0">Other</option>
                                        break;
                                }
                                  
                                </select>
                                </div>
                        </div>
                    </div>
                    <!--END Line5-->
<!--Fin lignes inversées-->
               
            <!--END section1-->
<!--AJOUT-->
            <!--Section2-->
            <div class="profile-info med" id="birthblock">
                <div class="row">
                <div class="form-group col-lg-3 col-sm-12">
                @if (Model.UserInformation.BirthDate == DateTime.MinValue)
                {
                    <div >
                    <label for="date-birth" class="text-info">Date of birth</label><br>
                    <input type='text' class="form-control" id='datetimepicker1' name="dateofbirth" value="" /><i class="fa fa-calendar uppercalendar" required></i>
                    </div>
                }
                else
                {
                    <div >
                    <label for="date-birth" class="text-info">Date of birth</label><br>
                    <input type='text' class="form-control" id='datetimepicker1' name="dateofbirth" value="@Model.UserInformation.BirthDate.ToShortDateString()" required /><i class="fa fa-calendar uppercalendar"></i>
                    </div>
                }
                    
                </div>
                <div class="form-group col-lg-3 col-sm-12">
                    <div>
                        <label for="birthcountry" class="text-info">Birth Country</label><br>
                        <input type="text" name="birthcountry" id="birthcountry" class="form-control" value="@Model.UserInformation.BirthCountry" required>
                    </div>
                </div>
                <div class="form-group col-lg-6 col-sm-12">
                    <div>
                        <label for="birthcity" class="text-info">Birth City</label><br>
                        <input type="text" name="birthcity" id="birthcity" class="form-control" value="@Model.UserInformation.BirthCity" required>
                    </div>
                </div>
                </div>
                        <div class="row">
                            <div class="form-group col-lg-6 col-sm-12">
                                <div>
                                <label for="mothersname" class="text-info">Mother's maiden name</label><br>
                                <input type="text" name="mothersname" id="mothersname" class="form-control" value="@Model.UserInformation.BirthMotherfullname" required>
                                </div>
                            </div>
                            <div class="form-group col-lg-6 col-sm-12">
                                <div>
                                <label for="fathersname" class="text-info">Father's name</label><br>
                                <input type="text" name="fathersname" id="fathersname" class="form-control" value="@Model.UserInformation.BirthFatherfullName" required>
                                </div>
                            </div>
                        </div>
                    </div>
            <!--END section2-->
            <!--Section3-->
            <div class="profile-info med">
                <div class="row">
                    <div class="col-lg-12 col-sm-12">
                            <div class="form-group">
                            <label for="documents-table" class="text-info">Upload your personal documents</label><br>
                            <div class="tg-wrap"><table id="documents-table" class="tg">
                                  <tr>
                                    <th class="tg-8lax">Document<span class="plus2"><a href="javascript:addDocument();"><img class="plus" src="/dashboard/assets/plus.png"></a></span></th>
                                    <th class="tg-7lax"></th>
                                  </tr>
                                  <tr>
                                    <td class="tg-8lax"><input type="text" name="filename_1" id="filename_1" class="form-control"  required></td>
                                    <td class="tg-7lax" style="text-align:center;">
                                    <input type="file" name="file_1" onchange="setFileName(this,'1');" id="fileSelector" style="display:none;" accept="image/jpeg,image/tiff,image/bmp,image/gif,image/png,application/pdf"  required/>
                                    <label for="fileSelector" class="btn btn-dark2 btn-info btn-lg text-right">Browse</label>
                                     
                                    @*<a href="" class="btn btn-orange btn-info btn-lg text-right">View</a>*@
                                    <span onclick="removeCurrentRow(this);"><a href="javascript:removeCurrentRow(this);">&nbsp;<img  class="trash" src="/dashboard/assets/trash.jpg"></a></span>
                                    </td>
                                  </tr>
                                </table>
                            </div>
                            </div>
                        </div>
                </div>
            </div>

            <!--END section3-->
            <!--Section4-->
            @if (Model.UserInformation.UserCertificationLevel > 2)
            {
                <div class="profile-info med" id="PinNeeded">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12"><p>Please enter a PIN between 4 to 8 characters/numbers. This PIN will be used to protect the use of your digital signature certificate and you will be required to enter it when you'll want to sign any document.</p></div>
