                                    <span><input type="radio" value="4" name="trustlevel" onchange="isPinNeeded();"> Level 4</span>
                                      break;
                                  case 1:
                                    <span><input type="radio" value="1" name="trustlevel" onchange="isPinNeeded();" disabled> Level 1</span><span style="Color:#ef4223;">(Current Trust Level)</span><br/>
                                    <span><input type="radio" value="2" name="trustlevel" onchange="isPinNeeded();" checked> Level 2</span><br/>
                                    <span><input type="radio" value="3" name="trustlevel" onchange="isPinNeeded();"> Level 3</span><br/>
                                    <span><input type="radio" value="4" name="trustlevel" onchange="isPinNeeded();"> Level 4</span>
                                      break;
                                  case 2:
                                    <span><input type="radio" value="1" name="trustlevel" onchange="isPinNeeded();" disabled> Level 1</span><br/>
                                    <span><input type="radio" value="2" name="trustlevel" onchange="isPinNeeded();" disabled> Level 2</span><span style="Color:#ef4223;">(Current Trust Level)</span><br/>
                                    <span><input type="radio" value="3" name="trustlevel" onchange="isPinNeeded();" checked> Level 3</span><br/>
                                    <span><input type="radio" value="4" name="trustlevel" onchange="isPinNeeded();"> Level 4</span>
                                      break;
                                  case 3:
                                    <span><input type="radio" value="1" name="trustlevel" onchange="isPinNeeded();" disabled> Level 1</span><br/>
                                    <span><input type="radio" value="2" name="trustlevel" onchange="isPinNeeded();" disabled> Level 2</span><br/>
                                    <span><input type="radio" value="3" name="trustlevel" onchange="isPinNeeded();" disabled> Level 3</span><span style="Color:#ef4223;">(Current Trust Level)</span><br/>
                                    <span><input type="radio" value="4" name="trustlevel" onchange="isPinNeeded();" checked> Level 4</span>
