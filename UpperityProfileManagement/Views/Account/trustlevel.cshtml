                            <label for="pin" class="text-info">Enter your PIN</label><br>
                            <input type="text" name="pin" id="pin" class="form-control"  required>
                        </div>
                        <div class="form-group col-xl-2 col-md-6 col-sm-12">
                            <label for="confirmpin" class="text-info">Confirm your PIN</label><br>
                            <input type="text" name="confirmpin" id="confirmpin" class="form-control"  required>
                        </div>
                    </div>
                </div>
            }
            else
            {
                <div class="profile-info med" id="PinNeeded" style="display:none;">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12"><p>Please enter a PIN between 4 to 8 characters/numbers. This PIN will be used to protect the use of your digital signature certificate and you will be required to enter it when you'll want to sign any document.</p></div>
                        <div class="form-group col-xl-2 col-md-6 col-sm-12">
                            <label for="pin" class="text-info">Enter your PIN</label><br>
                            <input type="text" name="pin" id="pin" class="form-control" pattern=".{4,8}" onchange="checkMatch();">
                        </div>
                        <div class="form-group col-xl-2 col-md-6 col-sm-12">
                            <label for="confirmpin" class="text-info">Confirm your PIN</label><br>
                            <input type="text" name="confirmpin" id="confirmpin" class="form-control" onchange="checkMatch();">
                        </div>
                    </div>
                </div>
            }
            <!--END section4-->
<!--FIN AJOUT-->
            <div class="form-group col-12 text-right">
                <input type="submit" name="submit" class="btn btn-dark btn-info btn-lg text-right" value="Cancel">
                  <input type="submit" name="submit" id="submitter" class="btn btn-orange btn-info btn-lg text-right" style="display: none;" value="Send to Upperity">
                <input type="button" name="" class="btn btn-orange btn-info btn-lg text-right" onclick="getInvoice();" value="Send to Upperity">
            </div>
