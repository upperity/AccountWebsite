<!--AJOUT-->

<script language="javascript" type="text/javascript">
    var indexer = 1;

    var phoneIndex = 150;
    var emailIndex = 150;
    function getInvoice() {

        if (isRequiredCompleted()) {
            $.ajax({
                type: 'POST',
                url: '/Account/GenerateInvoice',
                data: '{invoiceType: 1, invoiceLevel: "' + $("input[name='trustlevel']:checked").val() + '"}',
                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                async: false,
                success: function (data) {

                    window.open(data.invoiceURL, '_blank');
                    checkPaiementReceive(data.invoiceId);


                },
                error: function (xhr) {
                    alert('error');
                    alert(xhr.responseText);
                }
            });
        } else {
            $("#submitter").click();
        }
    }


    function checkPaiementReceive(invoiceId) {

        setTimeout(function () {
            $.ajax({
                type: 'POST',
                url: '/Account/checkPaiementReceive',
                data: '{invoiceId: "' + invoiceId + '"}',
                contentType: 'application/json; charset=utf-8',
                async: false,
                dataType: 'json',
                success: function (data) {
                    //$('#EntityDocumentList').html(data);
                    if (data.isPaid == true) {
                        $("#finalinvoiceId").val(invoiceId);
                        $("#submitter").click();
                    } else {
                        checkPaiementReceive(invoiceId);
                    }
                },
                error: function (xhr) {
                    alert('error');
                    alert(xhr.responseText);
                }
            });
        }, 5000);

    }

    function isRequiredCompleted() {
        var form = document.getElementById('trust-level');
        for (var i = 0; i < form.elements.length; i++) {
            if (form.elements[i].value === '' && form.elements[i].hasAttribute('required')) {

                return false;
            }
        }

        return true;
    }
    function addEmailField() {
        var newRow = '<tr><td class="tg-0lax"><input type="text" name="email_' + emailIndex + '" id="email_' + emailIndex + '" class="form-control" required></td><td class="tg-0lax"><select class="mdb-select md-form" name="emailtype_' + emailIndex + '" id="emailtype_' + emailIndex + '"><option value="1" selected>Personal</option><option value="2">Entreprise</option><option value="3">Other</option></select></td><td class="tg-0lax"><input type="radio" name="emailDefault" value="' + emailIndex + '" class="form-control"></td><td class="tg-0lax"><a href="#" onclick="removeCurrentRow(this);"><img class="trash" src="/dashboard/assets/trash.jpg"></a></td></tr>';
        $('#emails-table tbody').append(newRow);
        emailIndex++;
    }

    function addPhoneField() {
        var newRow = '<tr><td class="tg-0lax"><input type="text" name="phone_' + phoneIndex + '" id="phone_' + phoneIndex + '" class="form-control" required></td><td class="tg-0lax"><select class="mdb-select md-form" name="phoneType_' + phoneIndex + '" id="phoneType_' + phoneIndex + '"><option value="1" selected>Mobile</option><option value="2">Entreprise</option><option value="3">Other</option></select></td><td class="tg-0lax"><input type="radio" name="phoneDefault" class="form-control" value="' + phoneIndex + '"></td><td class="tg-0lax"><a href="#" onclick="removeCurrentRow(this);"><img class="trash" src="/dashboard/assets/trash.jpg"></a></td></tr>';
        $('#phones-table tbody').append(newRow);
        phoneIndex++;
    }

    function addDocument() {
        indexer++;
        var newRow = '<tr><td class="tg-8lax"><input type="text" name="filename_' + indexer + '" id="filename_' + indexer + '" class="form-control" required></td><td class="tg-7lax" style="text-align:center;"><input type="file" accept="image/jpeg,image/tiff,image/bmp,image/gif,image/png,application/pdf" name="file_' + indexer + '" onchange="setFileName(this,\'' + indexer + '\');" id="fileSelector' + indexer + '" style="display:none;" /><label for="fileSelector' + indexer + '" class="btn btn-dark2 btn-info btn-lg text-right">Browse</label><span onclick="removeCurrentRow(this);"><a href="javascript:removeCurrentRow(this);"><img  class="trash" src="/dashboard/assets/trash.jpg"></a></span></td></tr>';
        $('#documents-table tbody').append(newRow);
    }
    function removeCurrentRow(caller) {
        //$(caller).closest('tr').remove();
        $(caller).parent().parent().remove();
    }
    function setFileName(caller, identifier) {
        var pointer = "#filename_" + identifier;
        $(pointer).val(caller.value.split(/(\\|\/)/g).pop());
    }

    function isPinNeeded() {
        var level = $("input[name='trustlevel']:checked").val();
        if (level > 2) {
            $("#PinNeeded").css('display', '');
            $("#birthblock").css('display', '');

            $('#pin').prop('required', true);
            $('#confirmpin').prop('required', true);
            $('#datetimepicker1').prop('required', true);
            $('#birthcountry').prop('required', true);
            $('#birthcity').prop('required', true);
            $('#mothersname').prop('required', true);
            $('#fathersname').prop('required', true);
        } else {
            $("#PinNeeded").css('display', 'none');
            $("#birthblock").css('display', 'none');

            $('#pin').prop('required', false);
            $('#confirmpin').prop('required', false);
            $('#datetimepicker1').prop('required', false);
            $('#birthcountry').prop('required', false);
            $('#birthcity').prop('required', false);
            $('#mothersname').prop('required', false);
            $('#fathersname').prop('required', false);
        }

    }

    function checkMatch() {
        if ($("#pin").val() != $("#confirmpin").val()) {
            $('#confirmpin').get(0).setCustomValidity('Both PIN must match.');
        } else {
            $('#confirmpin').get(0).setCustomValidity('');
        }
    }

    function getCountryList() {
        $.ajax({
            type: 'POST',
            url: '/Account/GetCountryList',
            contentType: 'application/json; charset=utf-8',
            async: false,
            dataType: 'html',
            success: function (data) {
                $("#usercountry").html(data);
            },
            error: function (xhr) {
                alert('error');
                alert(xhr.responseText);
            }
        });
    }

    function getStateList() {
        $.ajax({
            type: 'POST',
            url: '/Account/GetStateList',
            contentType: 'application/json; charset=utf-8',
            dataType: 'html',
            async: false,
            data: '{CountryId: "' + $('#usercountry').val() + '"}',
            success: function (data) {
                $("#userState").html(data);
            },
            error: function (xhr) {
                alert('error');
                alert(xhr.responseText);
            }
        });
    }

    $(function () {
        $('#datetimepicker1').datetimepicker({
            viewMode: 'days',
            format: 'YYYY/MM/DD',
            maxDate: 'now'
        });

        getCountryList();
        $("#usercountry").val('@Model.UserInformation.AddressList[0].Country');
        getStateList();
        $("#userState").val('@Model.UserInformation.AddressList[0].ProvinceOrState');

        isPinNeeded();

    });
</script>
<!-- FIN ajout-->